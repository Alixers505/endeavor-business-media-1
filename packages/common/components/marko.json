{
  "taglib-imports": [
    "./elements/marko.json",
    "./blocks/marko.json",
    "./style-a/marko.json",
    "./style-b/marko.json"
  ],
  "<common-endeavor-styles>": {
    "template": "./endeavor-styles.marko"
  },
  "<common-table>": {
    "template": "./core-table.marko",
    "@border": "number",
    "@width": "string",
    "@class": "string",
    "@spacing": "number",
    "@padding": "number",
    "@valign": "string",
    "@align": "string",
    "@style": "string",
    "@attrs": "object"
  },
  "<common-dpm-emailx>": {
    "template": "./dpm-emailx.marko",
    "<dpm>": {
      "@enabled": "boolean",
      "@ln": "string",
      "@lc": "string",
      "@lcv": "string",
      "@lkw": "string"
    },
    "<ad-unit>": {
      "@uri": "string",
      "@id": "string",
      "@name": "string",
      "@alias": "string"
    },
    "<image>": {},
    "<link>": {},
    "<when-empty>": {},
    "@date": "date",
    "@use-emailx-href": "boolean",
    "@class": "string",
    "@attrs": "object"
  }
}
