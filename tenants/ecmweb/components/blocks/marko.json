{
  "<tenant-opt-out-block>": {
    "template": "./opt-out.marko"
  },
  "<tenant-section-spacer-block>": {
    "template": "./section-spacer.marko"
  },
  "<tenant-header-block>": {
    "template": "./header.marko",
    "@date": {
      "type": "object",
      "required": true
    },
    "@newsletter": {
      "type": "object",
      "required": true
    }
  },
  "<tenant-card-section-wrapper-block>": {
    "template": "./card-section-wrapper.marko",
    "@date": {
      "type": "object",
      "required": true
    },
    "@newsletter": {
      "type": "object",
      "required": true
    },
    "@section-id": {
      "type": "number",
      "required": true
    },
    "@limit": {
      "type": "number",
      "default-value": 24
    },
    "@title": "string",
    "@button-style": "string",
    "@title-style": "string",
    "@main-table-style": "string",
    "@content-link-style": "object"
  },
  "<tenant-list-section-wrapper-block>": {
    "template": "./list-section-wrapper.marko",
    "@date": {
      "type": "object",
      "required": true
    },
    "@newsletter": {
      "type": "object",
      "required": true
    },
    "@section-id": {
      "type": "number",
      "required": true
    },
    "@limit": {
      "type": "number",
      "default-value": 24
    },
    "@title": "string",
    "@title-style": "string",
    "@main-table-style": "string",
    "@content-link-style": "object"
  },
  "<tenant-simple-card-block>": {
    "template": "./simple-card.marko",
    "@node": "object",
    "@alignment": {
      "type": "string",
      "default-value": "left"
    },
    "@table-width": {
      "type": "number",
      "default-value": 345
    },
    "@img-width": {
      "type": "number",
      "default-value": 320
    },
    "@content-link-style": "object",
    "@teaser-style": "object",
    "@button-style": "string",
    "@button-text-style": "object"
  },
  "<tenant-simple-card-full-block>": {
    "template": "./simple-card-full.marko",
    "@node": "object",
    "@alignment": {
      "type": "string",
      "default-value": "left"
    },
    "@table-width": {
      "type": "number",
      "default-value": 345
    },
    "@img-width": {
      "type": "number",
      "default-value": 320
    },
    "@content-link-style": "object",
    "@teaser-style": "object",
    "@button-style": "string",
    "@button-text-style": "object"
  },
  "<tenant-promotion-block>": {
    "template": "./promotion.marko",
    "@node": "object",
    "@alignment": {
      "type": "string",
      "default-value": "left"
    },
    "@table-width": {
      "type": "number",
      "default-value": 345
    },
    "@img-width": {
      "type": "number",
      "default-value": 320
    },
    "@teaser-style": "object"
  },
  "<tenant-promotion-full-block>": {
    "template": "./promotion-full.marko",
    "@node": "object",
    "@alignment": {
      "type": "string",
      "default-value": "left"
    },
    "@table-width": {
      "type": "number",
      "default-value": 345
    },
    "@img-width": {
      "type": "number",
      "default-value": 320
    },
    "@teaser-style":  "object"
  }
}
