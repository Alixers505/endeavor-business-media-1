{
  "<tenant-header-block>": {
    "template": "./header.marko",
    "@date": {
      "type": "object",
      "required": true
    },
    "@logo-path" : {
      "type": "string",
      "default-value": "/files/base/ebm/tdworld/image/static/newsletter/header_logo.jpg"
    },
    "@header-href" : {
      "type": "string",
      "default-value": "https://www.tdworld.com/"
    },
    "@newsletter": {
      "type": "object",
      "required": true
    }
  },
  "<tenant-header-full-image-block>": {
    "template": "./header-full-image.marko",
    "@logo-path" : {
      "type": "string"
    },
    "@header-href" : {
      "type": "string",
      "default-value": "https://www.tdworld.com/"
    },
    "@newsletter": {
      "type": "object",
      "required": true
    }
  }
}
